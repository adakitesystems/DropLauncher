<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console">
<!--      <PatternLayout pattern="  %d{yyyy-MM-dd HH:mm:ss.SSS} [%C:%L] [%t] [%ex] [%-5level] [%logger{36}] [%msg]%n"/> -->
      <PatternLayout pattern="  %d{yyyy-MM-dd HH:mm:ss.SSS} [%C.%M:%L] [%ex] [%-5level] [%msg]%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
